{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "complexity": ["error", 30],
    "react/no-array-index-key": "off",
    "no-cond-assign": "warn",
    "react/jsx-key": [
      "error",
      {
        "checkFragmentShorthand": true,
        "checkKeyMustBeforeSpread": true,
        "warnOnDuplicates": true
      }
    ]
  }
}
